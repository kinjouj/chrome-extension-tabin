{
  "name": "tabin",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Tabin Chrome Extension",
  "browser_action": {},
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "storage",
    "http://*/", "https://*/"
  ]
}
